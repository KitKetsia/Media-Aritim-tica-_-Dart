//Declaração de variavéis
double MediaNota;

// Processamento dos dados e saída
void calcularMedia(int nota1,int nota2,int nota3,int nota4){
MediaNota = (nota1 + nota2 + nota3 + nota4) / 4;
  print ("Sua média é: $MediaNota");

  }

// Entrada de dados
void main() {
  calcularMedia(5,7,10,3);

}
